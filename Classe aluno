using System;

namespace Desafio__05
{
    public class Aluno
    {
        public static implicit operator string(Aluno aluno) => $"{aluno.Nome},{aluno.Nota}";
        public string Nome { get; set; }

        public double Nota { get; set; }

        public Aluno(string nome, double nota)
        {
            Nome = nome;

            Nota = nota;
        }

        public Aluno() { }

    }
}

